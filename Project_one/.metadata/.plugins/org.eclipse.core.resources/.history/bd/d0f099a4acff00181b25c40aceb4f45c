package com.rev.Main;

import java.io.IOException;
import java.sql.Connection;
import java.sql.SQLException;
import java.util.List;

import com.rev.Beans.Employee;
import com.rev.Dao.EmployeeDao;
import com.rev.DaoImpl.EmployeeDaoImpl;
import com.rev.Util.ConnectionUtil;

public class MainDriver {
	public static void main(String[] args) {
		init();
		EmployeeDao bd = new EmployeeDaoImpl();
		List<Employee> employee = bd.getEmployee();
		System.out.println(bd.getEmployee());
		for( Employee e : employee)
		{
		
		}

	}
	static void init() {
		try {
			Connection con = ConnectionUtil.getConnection("connection.properties");
			System.out.println(con);
		} catch (SQLException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
